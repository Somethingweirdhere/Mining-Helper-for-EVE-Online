<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="0dp"
    android:paddingLeft="0dp"
    android:paddingStart="9dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Space
            android:id="@+id/ooo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="169dp"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="30dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="3dp"
            android:text="@string/isk_per_m"
            app:layout_constraintLeft_toRightOf="@+id/ooo"
            app:layout_constraintTop_toBottomOf="@+id/PPI" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="6dp"
            android:text="ISK per item"
            app:layout_constraintLeft_toRightOf="@+id/ooo"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="2dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="3dp"
            android:text="ISK per hour"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/ooo"
            app:layout_constraintTop_toBottomOf="@+id/PPV"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/PPH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ISKS"
            android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/PPV"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="2dp"
            app:layout_constraintVertical_bias="1.0"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/PPV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ISKS"
            android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/PPI"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/PPI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ISKS"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="6dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/TV1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:text="Name"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>